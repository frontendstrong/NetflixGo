{
    "manifest_version": 2,
    "name": "Netflix Go",
    "short_name": "netflix-go",
    "version": "1.4",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [{
        "matches": ["https://www.netflix.com/*", "http://www.netflix.com/*"],
        "js": ["action.js"]
    }]
}